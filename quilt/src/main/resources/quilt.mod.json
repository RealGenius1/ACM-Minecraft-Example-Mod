{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "${mod_id}",
    "version": "${version}",
    "metadata": {
      "name": "${mod_name}",
      "description": "${description}",
      "contributors": {
        "${authors}": "Author"
      },
      "icon": "assets/acmexamplemod/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "org.acm.onu.quilt.ExampleModQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": "${minecraft_version}"
      },
      {
        "id": "architectury",
        "version": "${architectury_version}"
      }
    ]
  }
}
