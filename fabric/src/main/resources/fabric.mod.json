{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${authors}"
  ],
  "contact": {
    "homepage": "${homepage}",
    "issues": "${issues}",
    "sources": "${sources}"
  },
  "license": "${license}",
  "icon": "assets/acmexamplemod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.acm.onu.fabric.ExampleModFabric"
    ],
    "client": [
      "org.acm.onu.fabric.client.ExampleModFabricClient"
    ]
  },
  "depends": {
    "fabricloader": "${fabric_loader_version}",
    "minecraft": "${minecraft_version}",
    "java": "${java_version}",
    "architectury": "${architectury_version}",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
